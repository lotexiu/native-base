{
  "include": ["./src"],
  "exclude": ["**/*.test.tsx", "**/*.test.ts"],
  "compilerOptions": {
    "baseUrl": ".",
    "declaration": true,
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "lib": ["esnext", "dom"],
    "module": "esnext",
    "moduleResolution": "node",
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": false,
    "noImplicitUseStrict": false,
    "noStrictGenericChecks": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": false,
    "target": "esnext",
    "types": [],
    "noEmitOnError": false,
    "paths": {
      /* Specific paths need to be on top of this list */
      "@tsn-array/*": ["./src/utils/typescript/natives/array/*"],
      "@tsn-class/*": ["./src/utils/typescript/natives/class/*"],
      "@tsn-console/*": ["./src/utils/typescript/natives/console/*"],
      "@tsn-date/*": ["./src/utils/typescript/natives/date/*"],
      "@tsn-function/*": ["./src/utils/typescript/natives/function/*"],
      "@tsn-json/*": ["./src/utils/typescript/natives/json/*"],
      "@tsn-math/*": ["./src/utils/typescript/natives/math/*"],
      "@tsn-number/*": ["./src/utils/typescript/natives/number/*"],
      "@tsn-object/*": ["./src/utils/typescript/natives/object/*"],
      "@tsn-regex/*": ["./src/utils/typescript/natives/regex/*"],
      "@tsn-string/*": ["./src/utils/typescript/natives/string/*"],
      "@tsn/*": ["./src/utils/typescript/natives/*"],
      "@tsi/*": ["./src/utils/typescript/interfaces/*"],
      "@tsu/*": ["./src/utils/typescript/unknown/*"],
      "@tsrn/*": ["src/utils/typescript/react-native/*"],
      "@ts/*": ["./src/utils/typescript/*"],
      "native-base": ["./src/index"],
    },
  }
}
