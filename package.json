{
  "name": "native-base",
  "description": "Essential cross-platform UI components for React Native",
  "sideEffects": false,
  "keywords": [
    "android",
    "ios",
    "react",
    "native",
    "react-native",
    "native-modules",
    "react-component",
    "navbar",
    "packager"
  ],
  "scripts": {
    "release": "release-it",
    "test": "node_modules/.bin/jest",
    "typescript": "tsc --noEmit",
    "prepare": "npm run build",
    "build": "bob build",
    "build:types": "tsc --project tsconfig.build.json",
    "build:all": "npm run build && npm run build:types",
    "build:web": "rollup --config",
    "bundle:analysis": "visualize-bundle",
    "clean": "rm -rf node_modules && rm -rf lib && rm -rf package-lock.json",
    "clean-start": "npm run clean && npm install && npm run build"
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx}": [
      "eslint"
    ],
    "example/**/*.{js,ts,tsx}": [
      "eslint"
    ],
    "*.{json,md,html}": [
      "prettier --write"
    ]
  },
  "version": "3.4.28",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/src/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "typings": "lib/typescript/index.d.ts",
  "devDependencies": {
    "@commitlint/config-conventional": "^17.7.0",
    "@react-native-community/bob": "^0.17.1",
    "@react-native-community/eslint-config": "^3.2.0",
    "@release-it/conventional-changelog": "^7.0.0",
    "@testing-library/react-native": "^11.5.0",
    "@types/jest": "^29.5.5",
    "@types/lodash.clonedeep": "^4.5.9",
    "@types/lodash.get": "^4.4.9",
    "@types/lodash.has": "^4.5.9",
    "@types/lodash.isempty": "^4.4.9",
    "@types/lodash.isequal": "^4.5.8",
    "@types/lodash.isnil": "^4.0.9",
    "@types/lodash.merge": "^4.6.9",
    "@types/lodash.mergewith": "^4.6.9",
    "@types/lodash.omit": "^4.5.9",
    "@types/lodash.omitby": "^4.6.9",
    "@types/lodash.pick": "^4.4.9",
    "@types/lodash.uniqueid": "^4.0.9",
    "@types/node": "^20.8.0",
    "@types/react": "~18.0.27",
    "@types/react-native": "~0.69.0",
    "@types/tinycolor2": "^1.4.6",
    "@types/use-sync-external-store": "^0.0.6",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "commitlint": "^17.7.1",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "jest-expo": "~45.0.0",
    "lint-staged": "^14.0.1",
    "metro-react-native-babel-preset": "^0.72.3",
    "pod-install": "^0.1.39",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^3.0.3",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-native": "0.69.12",
    "react-native-safe-area-context": "4.3.1",
    "react-native-svg": "12.3.0",
    "react-native-web": "~0.18.9",
    "react-test-renderer": "18.0.0",
    "release-it": "^16.1.5",
    "rollup": "^3.29.4",
    "rollup-plugin-flow": "^1.1.1",
    "typescript": "~4.9.5",
    "visualize-bundle": "^1.4.0"
  },
  "peerDependencies": {
    "@types/react": "*",
    "@types/react-native": "*",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "react-native": ">=0.69.0 <0.75.0",
    "react-native-safe-area-context": ">=3.0.0",
    "react-native-svg": ">=12.0.0"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|@react-native-aria/.*|@react-stately/.*)"
    ],
    "setupFiles": [
      "<rootDir>/src/jest/mock.ts"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/example/*",
      "<rootDir>/lib/"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/",
    "cli.js",
    "example/webpack.config.js",
    "example/babel.config.js",
    "example/metro.config.js"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "@react-native-community/bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "commonjs"
      ],
      [
        "module"
      ]
    ]
  },
  "dependencies": {
    "@react-aria/visually-hidden": "^3.8.3",
    "@react-native-aria/button": "^0.2.10",
    "@react-native-aria/checkbox": "^0.2.3",
    "@react-native-aria/combobox": "^0.2.4-alpha.0",
    "@react-native-aria/focus": "^0.2.6",
    "@react-native-aria/interactions": "^0.2.2",
    "@react-native-aria/listbox": "^0.2.4-alpha.3",
    "@react-native-aria/overlays": "^0.3.3",
    "@react-native-aria/radio": "^0.2.4",
    "@react-native-aria/slider": "^0.2.5-alpha.1",
    "@react-native-aria/tabs": "^0.2.7",
    "@react-native-aria/utils": "^0.2.8",
    "@react-stately/checkbox": "^3.5.0",
    "@react-stately/collections": "^3.10.0",
    "@react-stately/combobox": "^3.6.0",
    "@react-stately/radio": "^3.9.0",
    "@react-stately/slider": "^3.4.0",
    "@react-stately/tabs": "^3.5.0",
    "@react-stately/toggle": "^3.6.0",
    "inline-style-prefixer": "^7.0.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.get": "^4.4.2",
    "lodash.has": "^4.5.2",
    "lodash.isempty": "^4.4.0",
    "lodash.isequal": "^4.5.0",
    "lodash.isnil": "^4.0.0",
    "lodash.merge": "^4.6.2",
    "lodash.mergewith": "^4.6.2",
    "lodash.omit": "^4.5.0",
    "lodash.omitby": "^4.6.0",
    "lodash.pick": "^4.4.0",
    "lodash.uniqueid": "^4.0.1",
    "stable-hash": "^0.0.4",
    "tinycolor2": "^1.6.0",
    "use-sync-external-store": "^1.2.0"
  },
  "directories": {
    "example": "example",
    "lib": "lib"
  },
  "gitHead": "5bbeacc403ba97622703699132c55d8359344004",
  "homepage": "https://github.com/GeekyAnts/NativeBase#readme",
  "readmeFilename": "README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GeekyAnts/NativeBase.git"
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/NativeBase"
  },
  "files": [
    "src",
    "lib",
    "cli.js"
  ]
}
